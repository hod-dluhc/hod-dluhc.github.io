{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": {
    "text": "UK Unemployment rate",
    "subtitle": "1971 - 2022, Ages 16 and over, % | Source: ONS",
    "fontSize": "14",
    "subtitleFontSize": "12",
    "color": "#000",
    "subtitleColor": "#000000"
  },
  "height": "300",
  "width": "400",
  "background": "#ffffff",
  "view": {
    "fill": "#ffffff"
  },
  "data": {
    "url": "https://api.economicsobservatory.com/gbr/unem?vega",
    "format": {
      "type": "json"
    }
  },
  "mark": {
    "type": "line",
    "color": "#f4134d",
    "strokeWidth": "2"
  },
  "encoding": {
    "x": {
      "field": "date",
      "type": "temporal",
      "axis": {
        "title": "",
        "titleColor": "#122B39"
      }
    },
    "y": {
      "field": "value",
      "type": "quantitative",
      "axis": {
        "title": "",
        "titleColor": "#122B39"
      }
    }
  }
}
